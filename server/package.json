{
  "name": "gptservices-server",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec 'node --loader ts-node/esm' ./src/http/index.ts | bunyan",
    "build": "tsc --project tsconfig.production.json",
    "test": "ava ./test/app ./test/http ./test/lib --timeout=1m",
    "test-debug": "ava ./test/debug --timeout=10m",
    "lint": "eslint . --ext .js,.ts --quiet"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "dependencies": {
    "await-limit": "^0.1.3",
    "bunyan": "^1.8.15",
    "firebase-admin": "^10.0.2",
    "http-errors": "^2.0.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-json-schema": "^3.0.1",
    "koa-router": "^10.1.1",
    "luxon": "^2.3.1",
    "mongodb": "^4.11.0",
    "openai": "^3.2.1",
    "require-env": "^0.2.1",
    "semver": "^7.3.8",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.6",
    "@types/koa-json": "^2.0.20",
    "@types/koa-router": "^7.4.4",
    "@types/luxon": "^2.3.1",
    "@types/node": "^18.11.4",
    "@types/semver": "^7.3.13",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "ava": "^4.1.0",
    "eslint": "^8.27.0",
    "node-mocks-http": "^1.12.1",
    "nodemon": "^2.0.20",
    "sinon": "^13.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.2"
  }
}
